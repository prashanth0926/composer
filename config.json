{
  "folderName": "MyBot",
  "local": [
    {
      "name": "mysql",
      "run": "<your mysql path>/support-files/mysql.server start"
    },
    {
      "name": "mongo",
      "run": "mongod"
    }
  ],
  "git": [
    {
      "gitRepo": "https://github.com/cagataygurturk/spring-boot-boilerplate.git",
      "apps": [
        {
          "name": "spring-app",
          "branch": "master",
          "path": "spring-boot-boilerplate/",
          "run": "mvn spring-boot:run"
        }
      ]
    },
    {
      "gitRepo": "https://github.com/tngan/loopback-redux-react-boilerplate.git",
      "apps": [
        {
          "name": "loopback-redux-react-app",
          "branch": "master",
          "path": "loopback-redux-react-boilerplate/",
          "dependencies": ["npm install"],
          "run": "npm start"
        }
      ]
    },
    {
      "gitRepo": "https://github.com/ngx-rocket/starter-kit.git",
      "apps": [
        {
          "name": "ng-rocket-app",
          "branch": "master",
          "path": "starter-kit/",
          "dependencies": ["npm install"],
          "run": "npm start"
        }
      ]
    }
  ]
}